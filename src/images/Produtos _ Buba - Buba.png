/* Waiting Spinnner */

.nld-chatbot .nld-input .nld-spinner {
  position: relative;
  width: 2em;
  height: 2em;

  top: 50%;
  left: 50%;
  margin-left: -1em;
  margin-top: -1em;
}

.nld-chatbot .nld-input .nld-spinner div {
  position: absolute;
  border-width: 0.25em;
  border-style: solid;
  border-color: inherit;
  margin-top: -0.25em;
  border-radius: 50% !important;
  animation: nld-spinner 1.6s cubic-bezier(0, 0.35, 0.28, 0.9) infinite;
}

.nld-chatbot .nld-input .nld-spinner div:nth-child(2) {
  animation-delay: -0.64s;
}

@keyframes nld-spinner {
  0% {
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    opacity: 1;
  }
  80% {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
  }
  100% {
    opacity: 0;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
  }
}

